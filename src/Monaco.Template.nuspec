<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2012/06/nuspec.xsd">
	<metadata>
		<id>Monaco.Template.Solution</id>
		<version>1.0.0-RC2</version>
		<description>
			Solution Template for .NET projects
		</description>
		<authors>One Beyond</authors>
		<license type="expression">ISC</license>
		<icon>Content\.template.config\icon.png</icon>
		<packageTypes>
			<packageType name="Template" />
		</packageTypes>
		<readme>docs\README.md</readme>
	</metadata>
	<files>
		<file src="Content\**\*.*"
			  exclude="Content\**\bin\**\*.*;Content\**\obj\**\*.*;Content\**\logs\**\*.*;Content\.vs\**\*.*;Content\.idea\*.*;Content\*.user"
			  target="Content" />
		<file src="..\README.md"
			  target="docs\" />
	</files>
</package>